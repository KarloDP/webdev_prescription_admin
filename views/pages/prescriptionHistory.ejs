<link rel="stylesheet" href="/css/prescriptionHistory.css">
<h1>Dispense History for Prescription #<%= prescriptionID %></h1>
<table>
  <thead>
    <tr>
      <th>ID</th><th>Date Dispensed</th><th>Medication</th><th>Quantity</th><th>Pharmacy</th>
    </tr>
  </thead>
  <tbody>
    <% if (history && history.length > 0) { %>
      <% history.forEach(h => { %>
        <tr>
          <td data-label="ID"><%= h.dispenseID %></td>
          <td data-label="Date Dispensed"><%= h.dispenseDate %></td>
          <td data-label="Medication"><%= h.brandName %></td>
          <td data-label="Quantity"><%= h.dispensedQuantity %></td>
          <td data-label="Pharmacy"><%= h.pharmacyName %></td>
        </tr>
      <% }) %>
    <% } else { %>
      <tr><td colspan="5">No dispense history found.</td></tr>
    <% } %>
  </tbody>
</table>
<a href="/prescriptions" class="back-btn">Back to Prescriptions</a>
